{
  "name": "canopy-sight",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "type-check": "turbo run type-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "turbo run clean && node -e \"require('fs').rmSync('node_modules',{recursive:true,force:true})\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.11.0",
    "@vitest/coverage-v8": "^1.1.0",
    "prettier": "^3.1.1",
    "puppeteer": "^24.36.1",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "packageManager": "npm@9.9.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {}
}
